{
  "name": "msw-scenarios",
  "version": "0.1.6",
  "description": "A scenario-based mocking library built on msw",
  "main": "dist/msw-scenarios.umd.js",
  "module": "dist/msw-scenarios.es.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "vite build",
    "prepare": "pnpm run build",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manofbackend/msw-scenarios.git"
  },
  "keywords": [
    "msw",
    "mocking",
    "typescript",
    "scenario-based"
  ],
  "author": "hyunBell",
  "license": "MIT",
  "homepage": "https://github.com/manofbackend/msw-scenarios#readme",
  "peerDependencies": {
    "msw": "^2.6.6"
  },
  "dependencies": {
    "immer": "^10.1.1",
    "msw": "^2.6.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^4.5.4",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^2.0.0"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
