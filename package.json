{
  "name": "msw-scenarios",
  "version": "0.3.0",
  "description": "A monorepo for MSW scenario-based mocking library and React DevTools",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/examples/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @msw-scenarios/core build",
    "build:devtools": "pnpm --filter @msw-scenarios/react-devtools build",
    "dev": "pnpm --filter @msw-scenarios/react-devtools dev",
    "test": "pnpm -r test",
    "test:core": "pnpm --filter @msw-scenarios/core test",
    "test:e2e": "pnpm --filter @msw-scenarios/core test:e2e",
    "test:e2e:ui": "pnpm --filter @msw-scenarios/core test:e2e:ui",
    "test:e2e:dev": "pnpm --filter @msw-scenarios/core test:e2e:dev",
    "test:e2e:install": "pnpm --filter @msw-scenarios/core test:e2e:install",
    "example:dev": "pnpm --filter @msw-scenarios/example-nextjs-app-router dev",
    "example:build": "pnpm --filter @msw-scenarios/example-nextjs-app-router build",
    "example:start": "pnpm --filter @msw-scenarios/example-nextjs-app-router start",
    "example:lint": "pnpm --filter @msw-scenarios/example-nextjs-app-router lint",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "prettier --write \"packages/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"packages/**/*.{js,jsx,ts,tsx,json,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manofbackend/msw-scenarios.git"
  },
  "keywords": [
    "msw",
    "mocking",
    "typescript",
    "scenario-based"
  ],
  "author": "hyunBell",
  "license": "MIT",
  "homepage": "https://github.com/manofbackend/msw-scenarios#readme",
  "peerDependencies": {
    "msw": "^2.6.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "immer": "^10.1.1",
    "msw": "^2.6.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@playwright/test": "^1.49.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "next": "^15.0.4",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.16",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^2.0.0"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
